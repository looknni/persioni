#!/bin/sh
echo '\$1,file'
source /root/fu.l
if [ -z $1 ];then
	exit
fi
aff=`cat $1`
if [ ! -f $1 ];then
	exit
fi
for i in $aff;do
if [ ! -z $i ];then
	iptables-nft -t mangle -A MEE -d $i -p all -j DROP 2>/dev/null
	fa=$?
	ip6tables-nft -t mangle -A MEE -d $i -p all -j DROP 2>/dev/null
	fa6=$?
	echo $i $fa '?' $fa6 '++'
fi
done
unset fa fa6 aff
addl4
addl6
uniq4
uniq6
